<?xml version="1.0" encoding="UTF-8"?>
<launch>
    <arg name="input_photo_path"  default="$(find camera_lidar_calibration)/../../data/photo/0.bmp" />  <!-- photo to find the corner -->
  
    <param name="input_photo_path"  value="$(arg input_photo_path)" />
    <param name="intrinsic_path"    value="$(find camera_lidar_calibration)/../../data/parameters/intrinsic.txt" />
    <param name="ouput_path"        value="$(find camera_lidar_calibration)/../../data/corner_photo.txt" />
    <node pkg="camera_lidar_calibration" name="cornerPhoto" type="cornerPhoto" output="screen" required="true"></node>

</launch>

